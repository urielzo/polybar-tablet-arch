#!/bin/sh
#
# Credit to this guy
#   https://www.reddit.com/r/minimalcatart/comments/cvqukg/smol_ket/
#


#bb="\033[48;05;232m"
#fb="\033[38;05;232m"

#bg="\033[48;05;242m"
#fg="\033[38;05;242m"

#bw="\033[48;05;252m"
#fw="\033[38;05;252m"

#br="\033[48;05;161m"
#fr="\033[38;05;161m"

#by="\033[48;05;178m"

e="\033[48;05;232m"

bb="\033[100m"
fb="\033[90m"

fg="\033[37m"
bg="\033[47m"

by="\033[43m"

br="\033[41m"
fr="\033[31m"

bw="\033[107m"
fw="\033[97m"

c="\033[0m"

# █ ▄ ▀


printf "
$bb$fg ▄$c$fb▄▄$bb$fg▄ $c
$bb$fg $e▀$fw█$e$fg▀█$bb $c
$fb▀$c$bb$fw▀$by▀$br▀$c$fr$bw▀$fg▀$c$fb$bg▀▀▀$c$fb▄$c $fb▄$fg$bb▄$c$fb▄
 $fb█$fw$bw▀$bb▀$bw▀$bg▀$c$fg███$c$fb$bg▄$c$bg$fb▀▀$c$fg$bb▀$c$fb▀
 $fb▀$fw▀$fb▀$fw▀$fb▀$fg▀$fb▀$fg▀$fb▀$fb▀▀$c

"
